<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".perfilFragment">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/GrisApp"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="-20dp">

        <TextView
            android:id="@+id/lblWinRatio"
            android:layout_width="61dp"
            android:layout_height="21dp"
            android:layout_marginTop="500dp"
            android:text="WinRatio"
            android:textColor="#000000"
            android:textSize="14sp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.954"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/lblWinRatioUsuario"
            android:layout_width="41dp"
            android:layout_height="20dp"
            android:layout_marginTop="528dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.929"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/lblVictorias"
            android:layout_width="51dp"
            android:layout_height="19dp"
            android:layout_marginTop="500dp"
            android:text="Victorias"
            android:textColor="#000000"
            android:textSize="14sp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.044"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/lblNºVictorias"
            android:layout_width="41dp"
            android:layout_height="20dp"
            android:layout_marginTop="528dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.04"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/lblDerrotas"
            android:layout_width="51dp"
            android:layout_height="19dp"
            android:layout_marginTop="500dp"
            android:text="Derrotas"
            android:textColor="#000000"
            android:textSize="14sp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/lblNºDerrotas"
            android:layout_width="41dp"
            android:layout_height="20dp"
            android:layout_marginTop="528dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.486"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/lblPais"
            android:layout_width="51dp"
            android:layout_height="19dp"
            android:layout_marginTop="360dp"
            android:text="País"
            android:textColor="#000000"
            android:textSize="14sp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.044"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="@+id/imgBannner" />

        <TextView
            android:id="@+id/textView29"
            android:layout_width="141dp"
            android:layout_height="26dp"
            android:layout_marginTop="420dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.055"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/lblFechaNacimiento"
            android:layout_width="144dp"
            android:layout_height="17dp"
            android:layout_marginTop="312dp"
            android:text="Fecha de nacimiento"
            android:textColor="#000000"
            android:textSize="14sp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.051"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/lblUsuario"
            android:layout_width="51dp"
            android:layout_height="19dp"
            android:layout_marginTop="220dp"
            android:text="Usuario"
            android:textColor="#000000"
            android:textSize="14sp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.041"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <ImageView
            android:id="@+id/imgBannner"
            android:layout_width="507dp"
            android:layout_height="157dp"
            android:layout_marginTop="40dp"
            android:src="@drawable/fondobanner"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <ImageView
            android:id="@+id/imgUsuario"
            android:layout_width="111dp"
            android:layout_height="111dp"
            android:layout_marginTop="60dp"
            android:src="@drawable/wellplayed"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/lblUsuario2"
            android:layout_width="124dp"
            android:layout_height="22dp"
            tools:layout_editor_absoluteX="14dp"
            tools:layout_editor_absoluteY="240dp" />

        <TextView
            android:id="@+id/lblFechaNacimiento2"
            android:layout_width="122dp"
            android:layout_height="22dp"
            tools:layout_editor_absoluteX="16dp"
            tools:layout_editor_absoluteY="330dp" />

    </androidx.constraintlayout.widget.ConstraintLayout>
</FrameLayout>